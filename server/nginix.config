worker_processes 1;
events{
    worker_connections 1024;
}
http{
    sendfile on;

    upstream app_backend{
        server 127.0.0.1:8000;
    }

    server{
        listen 8080;
        server_name localhost

        location /{
            proxy_pass http://app_backend
            proxy_set_header Host $host;
            proxy_set_header X-Real-IP $remote_addr;
            proxy_set
        }
    }
}